<% if user.profile_image_id != nil %>
  <%= attachment_image_tag user, :profile_image, size:"50x50", class:"rounded", fallback: "no_image.jpg" %>
<% elsif user.profile_image_id == nil && user.provider != nil %>
  <%= user.image %>
<% else %>
  <%= attachment_image_tag user, :profile_image, size:"50x50", class:"rounded", fallback: "no_image.jpg" %>
<% end %>
